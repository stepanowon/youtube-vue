{
  "name": "youtube-vue",
  "version": "2.0.13",
  "description": "Youtube player component for vue.js 2.x",
  "private": false,
  "main": "dist/youtube-vue.common.js",
  "unpkg": "dist/youtube-vue.umd.js",
  "scripts": {
    "serve": "cross-env NODE_OPTIONS=--openssl-legacy-provider vue-cli-service serve",
    "build": "cross-env NODE_OPTIONS=--openssl-legacy-provider vue-cli-service build",
    "build:lib": "cross-env NODE_OPTIONS=--openssl-legacy-provider vue-cli-service build --target lib --filename youtube-vue --name YoutubeVue src/index.js",
    "lint": "cross-env NODE_OPTIONS=--openssl-legacy-provider vue-cli-service lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yazinsai/youtube-vue.git"
  },
  "keywords": [
    "Youtube",
    "Player",
    "Vue.js"
  ],
  "author": "Stephen Won",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yazinsai/youtube-vue/issues"
  },
  "homepage": "https://github.com/yazinsai/youtube-vue",
  "dependencies": {
    "core-js": "^3.8.3",
    "cross-env": "^7.0.3",
    "shortid": "^2.2.16",
    "vue": "^2.6.14",
    "youtube-player": "^5.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "^3.3.1",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "vue-template-compiler": "^2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
